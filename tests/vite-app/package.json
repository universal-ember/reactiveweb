{
  "name": "vite-app",
  "version": "0.0.0",
  "private": true,
  "description": "Test app for reactiveweb addon",
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "vite build",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:types": "ember-tsc --noEmit",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:prettier": "prettier -c .",
    "lint:prettier:fix": "prettier -w .",
    "_syncPnpm": "pnpm sync-dependencies-meta-injected",
    "start": "vite",
    "test:ember": "vite build --mode development && ember test --path dist"
  },
  "dependenciesMeta": {
    "reactiveweb": {
      "injected": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/plugin-transform-runtime": "^7.26.9",
    "@babel/plugin-transform-typescript": "^7.28.0",
    "@ember/optional-features": "^2.0.0",
    "@ember/string": "^4.0.0",
    "@ember/test-helpers": "^5.4.1",
    "@embroider/compat": "^4.0.3",
    "@embroider/config-meta-loader": "^1.0.0",
    "@embroider/core": "^4.0.3",
    "@embroider/vite": "^1.1.1",
    "@glimmer/component": "^2.0.0",
    "@glimmer/tracking": "^1.1.2",
    "@glint/ember-tsc": "^1.0.8",
    "@glint/template": "^1.6.1",
    "@glint/tsserver-plugin": "^2.0.8",
    "@rollup/plugin-babel": "^6.1.0",
    "@tsconfig/ember": "^3.0.1",
    "@types/qunit": "^2.19.13",
    "@types/rsvp": "^4.0.6",
    "babel-plugin-ember-template-compilation": "^3.0.1",
    "concurrently": "^9.2.1",
    "decorator-transforms": "^2.3.0",
    "ember-auto-import": "^2.12.0",
    "ember-cli": "~6.9.1",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-htmlbars": "^7.0.0",
    "ember-load-initializers": "^3.0.0",
    "ember-modifier": "^4.2.2",
    "ember-page-title": "^9.0.3",
    "ember-qunit": "^9.0.4",
    "ember-resolver": "^13.0.0",
    "ember-source": "~6.9.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^7.9.3",
    "ember-try": "^4.0.0",
    "eslint": "^9.37.2",
    "graphql": "^16.9.0",
    "pnpm-sync-dependencies-meta-injected": "^0.0.14",
    "prettier": "^3.0.3",
    "qunit": "^2.20.0",
    "qunit-dom": "^3.0.0",
    "reactiveweb": "workspace:*",
    "tracked-built-ins": "^4.0.0",
    "typescript": "^5.6.3",
    "vite": "^7.3.0"
  },
  "engines": {
    "node": ">= 22"
  },
  "ember": {
    "edition": "octane"
  },
  "dependencies": {
    "@ember/test-waiters": "^4.1.1",
    "@embroider/macros": "^1.19.5",
    "@nullvoxpopuli/eslint-configs": "^5.5.0",
    "ember-concurrency": "^5.0.0",
    "ember-modify-based-class-resource": "^1.1.0",
    "ember-resources": "^6.5.1",
    "msw": "2.4.7"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "ember-addon": {
    "type": "app",
    "version": 2
  },
  "exports": {
    "./tests/*": "./tests/*",
    "./*": "./app/*"
  }
}
