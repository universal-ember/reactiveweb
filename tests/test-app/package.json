{
  "name": "test-app",
  "version": "0.0.0",
  "private": true,
  "description": "Test app for reactiveweb addon",
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:types": "ember-tsc --noEmit",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:prettier": "prettier -c .",
    "lint:prettier:fix": "prettier -w .",
    "_syncPnpm": "pnpm sync-dependencies-meta-injected",
    "start": "concurrently 'ember serve' 'pnpm _syncPnpm --watch' --names 'tests serve,tests sync deps'",
    "test:ember": "pnpm _syncPnpm && ember test --test-port 0"
  },
  "dependenciesMeta": {
    "reactiveweb": {
      "injected": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@ember/optional-features": "^2.0.0",
    "@ember/string": "^3.1.1",
    "@ember/test-helpers": "^4.0.2",
    "@embroider/compat": "^3.6.1",
    "@embroider/core": "^3.4.15",
    "@embroider/webpack": "^4.0.5",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@glint/ember-tsc": "^1.0.8",
    "@glint/template": "^1.6.1",
    "@glint/tsserver-plugin": "^2.0.8",
    "@tsconfig/ember": "^3.0.1",
    "@types/qunit": "^2.19.7",
    "@types/rsvp": "^4.0.6",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^9.0.1",
    "ember-auto-import": "^2.8.0",
    "ember-cli": "~5.4.0",
    "ember-cli-app-version": "^6.0.1",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-load-initializers": "^2.1.2",
    "ember-modifier": "^4.1.0",
    "ember-page-title": "^8.2.0",
    "ember-qunit": "^8.0.2",
    "ember-resolver": "^11.0.1",
    "ember-source": "~5.4.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-imports": "^4.1.1",
    "ember-template-lint": "^6.0.0",
    "ember-try": "^3.0.0",
    "eslint": "^9.37.2",
    "graphql": "^16.9.0",
    "loader.js": "^4.7.0",
    "pnpm-sync-dependencies-meta-injected": "^0.0.14",
    "prettier": "^3.0.3",
    "qunit": "^2.20.0",
    "qunit-dom": "^3.0.0",
    "reactiveweb": "workspace:*",
    "tracked-built-ins": "^3.3.0",
    "typescript": "^5.6.3",
    "webpack": "^5.89.0"
  },
  "engines": {
    "node": ">= 22"
  },
  "ember": {
    "edition": "octane"
  },
  "dependencies": {
    "@ember/test-waiters": "^3.1.0",
    "@embroider/macros": "^1.16.6",
    "@nullvoxpopuli/eslint-configs": "^5.5.0",
    "ember-concurrency": "^3.1.1",
    "ember-modify-based-class-resource": "^1.1.0",
    "ember-resources": "^6.5.1",
    "msw": "2.4.7"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
