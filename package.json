{
  "name": "reactiveweb",
  "version": "1.9.1",
  "description": "Web platform primitives and utilities for building reactive applications",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/universal-ember/reactiveweb",
  "license": "MIT",
  "author": "NullVoxPopuli",
  "exports": {
    ".": {
      "types": "./declarations/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./declarations/*.d.ts",
      "default": "./dist/*.js"
    },
    "./resource/modifier": {
      "types": "./declarations/resource/modifier/index.d.ts",
      "default": "./dist/resource/modifier/index.js"
    },
    "./addon-main.js": "./addon-main.cjs"
  },
  "imports": {
    "#types": "./src/-private/types.ts"
  },
  "typesVersions": {
    "*": {
      "*": [
        "declarations/*"
      ]
    }
  },
  "files": [
    "addon-main.cjs",
    "declarations",
    "dist"
  ],
  "scripts": {
    "build": "concurrently 'npm:build:*'",
    "build:js": "rollup --config",
    "build:types": "ember-tsc --declaration --declarationDir declarations",
    "lint": "concurrently 'npm:lint:*(!fix)' --names 'lint:'",
    "lint:fix": "concurrently 'npm:lint:*:fix' --names 'fix:'",
    "lint:hbs": "ember-template-lint . --no-error-on-unmatched-pattern",
    "lint:hbs:fix": "ember-template-lint . --fix --no-error-on-unmatched-pattern",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:prettier": "prettier -c .",
    "lint:prettier:fix": "prettier -w .",
    "lint:types": "ember-tsc --noEmit",
    "start": "concurrently 'npm:start:*'",
    "start:js": "rollup --config --watch --no-watch.clearScreen",
    "start:types": "ember-tsc --declaration --watch",
    "test": "echo 'A v2 addon does not have tests, run tests in test-app'"
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.8.6",
    "@embroider/macros": "^1.16.6",
    "decorator-transforms": "^2.2.2",
    "ember-resources": ">= 6.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.23.7",
    "@babel/plugin-transform-class-static-block": "^7.23.4",
    "@babel/plugin-transform-typescript": "^7.23.6",
    "@babel/runtime": "^7.23.8",
    "@ember/library-tsconfig": "^2.0.0",
    "@ember/test-waiters": "^4.0.0",
    "@embroider/addon-dev": "^8.0.0",
    "@glint/ember-tsc": "^1.0.8",
    "@glint/template": "^1.6.1",
    "@glint/tsserver-plugin": "^2.0.8",
    "@nullvoxpopuli/eslint-configs": "^5.5.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@tsconfig/ember": "^3.0.1",
    "babel-plugin-ember-template-compilation": "^4.0.0",
    "concurrently": "^9.0.1",
    "ember-source": "~6.10.0",
    "ember-template-lint": "^7.0.0",
    "eslint": "^9.39.2",
    "expect-type": "^1.2.2",
    "prettier": "^3.7.4",
    "prettier-plugin-ember-template-tag": "^2.1.2",
    "rollup": "^4.21.3",
    "rollup-plugin-copy": "^3.5.0",
    "typescript": "^5.6.3"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.cjs"
  },
  "peerDependencies": {
    "@ember/test-waiters": ">= 3.1.0"
  }
}
